{
  "name": "food-ordering-system",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.15",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "express": "^5.1.0",
    "framer-motion": "^12.23.24",
    "lightswind": "^3.1.18",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "serve": "npm run backend:build && cd backend && npm start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "backend:install": "cd backend && npm install",
    "backend:dev": "cd backend && npm run dev",
    "backend:build": "cd backend && npm run build",
    "backend:migrate": "cd backend && npm run migrate",
    "backend:add-order-fields": "cd backend && npm run add-order-fields",
    "backend:seed": "cd backend && npm run seed",
    "backend:clean-inventory": "cd backend && npm run clean-inventory",
    "backend:remove-duplicates": "cd backend && npm run remove-duplicates",
    "dev": "concurrently \"npm start\" \"npm run backend:dev\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "tailwindcss": "^3.4.18",
    "concurrently": "^8.2.2"
  }
}
